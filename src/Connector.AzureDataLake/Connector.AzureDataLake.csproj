<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>CluedIn.Connector.AzureDataLake provides the ability to post data to Azure DataLake</Description>
    <AssemblyName>CluedIn.Connector.AzureDataLake</AssemblyName>
    <RootNamespace>CluedIn.Connector.AzureDataLake</RootNamespace>
    <IsCluedInServerComponent>true</IsCluedInServerComponent>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="Resources\azuredatalake.png" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Resources\azuredatalake.png" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Azure.Messaging.EventHubs" />
    <PackageReference Include="Azure.Storage.Files.DataLake" />
    <PackageReference Include="CluedIn.Connector.Common" />
    <PackageReference Include="CluedIn.Core" />
    <PackageReference Include="CluedIn.Core.Agent" />
    <PackageReference Include="CluedIn.Crawling" />
    <PackageReference Include="ComponentHost" />
    <PackageReference Include="Microsoft.AspNet.WebApi.Client" />
    <PackageReference Include="Microsoft.Azure.Management.DataLake.Store" />
  </ItemGroup>

